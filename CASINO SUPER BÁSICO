apuesta=input("Apuestas a PAR o IMPAR -->")
import random
resultado = random.randint(0, 36)
print("SALIÃ“ EL:") 
print(resultado)

if resultado == 0:
    FINAL="CERO"
if resultado % 2 == 0:
    FINAL="PAR"
else:
    FINAL="IMPAR"
print(FINAL)
if(apuesta==FINAL):
    print("HAS GANADO")
if(apuesta!=FINAL):
    print("HAS PERDIDO")
